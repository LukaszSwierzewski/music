
<% include partials/header.ejs %>  

<div class="container">
<h2 class="form-group newejs">Zarejestruj konto</h2>
<div>
<form action="/register", method="POST">
    <div class="col-md-4 col-md-offset-4">
    <div class='form-group'>
    <input class="form-control" type="text" name='username' placeholder="Login">
    </div>
      <div class="form-group">
    <input class="form-control" type="password" name='password' placeholder="Hasło">
    </div class='form-group'>
    <div class='form-group'>
    <h2 class='text-center'>Dane poniżej nie są wymagane</h2>
    <div>
    <input class="form-control" type="text" name='avatar' placeholder="Twój avatar">
    </div>
    <div class='form-group'>
    <input class="form-control" type="text" name='firstName' placeholder="Imie">
    </div>
    <div class="form-group">
    <input class="form-control" type="text" name='lastName' placeholder="Nazwisko">
    </div>
    <div class="form-group">
    <input class="form-control" type="email" name='email' placeholder="email">
    </div>
       <div class="form-group">
    <input class="form-control" type="password" name='adminCode' placeholder="admin code">
    </div>
    <button class="btn btn-lg btn-primary btn-block">Rejestruj</button>
</form>


<p class='newejse'><a href="/index">Powrót</a></p>
</div>
</div>
</div>
</div>

